
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  user_id    Int       @id @default(autoincrement())
  username   String
  email      String    @unique
  password   String
}

model Admin {
  admin_id   Int       @id @default(autoincrement())
  username   String
  email      String    @unique
  password   String
}